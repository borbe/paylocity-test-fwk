{
  "extends": "eslint-config-airbnb",
  "rules": {
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0,
    "no-await-in-loop":0,
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "no-restricted-syntax":0,
    "no-mixed-operators":0,
    "consistent-return":0,
    "default-case": 0,
    "no-restricted-properties": 0,
    "global-require":0,
    "import/no-dynamic-require":0,
    "no-underscore-dangle":0,
    "guard-for-in":0
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "browser": true,
    "element": true,
    "by": true,
    "expect": true,
    "afterAll": true,
    "beforeAll": true,
    "protractor": true,
    "angular": true,
    "jasmine": true,
    "localStorage": true,
    "pageMap":true,
    "until":true,
    "webdriver":true
  }
}